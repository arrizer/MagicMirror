{
  "port": 8080,
  "language": "en",
  "widgets": [
    {
      "widget": "clock",
      "config" : {
        "locale": "en"
      }
    },
    {
      "widget": "birthdays",
      "config": {
        "server": "https://mycarddavserver.com/card.php",
        "username": "xxxxxx",
        "password": "xxxxxx",
        "daysAhead": 0
      }
    },
    { 
      "widget": "news",
      "config": {
        "feeds" : [
          "http://www.spiegel.de/schlagzeilen/tops/index.rss"
        ]
      }
    },
    {
      "widget": "netatmo",
      "config": {
        "auth": {
          "client_id": "xxxxxxxxx",
          "client_secret": "xxxxxxxxx",
          "username": "your@email.com",
          "password": "xxxxxxxxxx"
        },
        "stations": ["Home"]
      }
    },
    {
      "widget": "rain",
      "config": {
        "latitude": 48.1234,
        "longitude": 11.1234
      }
    },
    {
      "widget": "weather",
      "config": {
        "wunderground_api_key": "xxxxxxxxx",
        "country": "Germany",
        "city": "Munich",
        "units": "celsius"
      }
    },
    {
      "widget": "stocks",
      "config": {
        "quotes": [
          {
            "symbol": "AAPL",
            "title": "Apple"
          }
        ]
      }
    },
    {
      "widget": "mvglive",
      "config": {
        "mvglive_api_key": "xxxxxxxxx",
        "stations": [
          "Marienplatz"
        ]
      }
    },
    {
      "widget": "iobroker",
      "config": {
        "baseURL": "http://myserver.local:8888",
        "views": [
          {
            "view": "openWindows",
            "objects": {
              "Door 1": "xxx",
              "Door 2": "yyy"
            }
          }
        ]
      }
    }
  ]
}
